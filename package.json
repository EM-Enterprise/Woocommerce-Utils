{
  "name": "@em-enterprise/woocommerce-utils",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "jest --coverage",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --options typedoc.json",
    "semantic-release": "semantic-release"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./schemas/*": {
      "import": "./dist/schemas/*.js",
      "require": "./dist/schemas/*.cjs",
      "types": "./dist/schemas/*.d.ts"
    },
    "./internal/functions/*": {
      "import": "./dist/functions/*.js",
      "require": "./dist/functions/*.cjs",
      "types": "./dist/functions/*.d.ts"
    },
    "./internal/api/*": {
      "import": "./dist/api/*.js",
      "require": "./dist/api/*.cjs",
      "types": "./dist/api/*.d.ts"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "bottleneck": "^2.19.5",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "fetch": "^1.1.0",
    "semantic-release": "^24.2.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/jest-expect-message": "^1.1.0",
    "@types/node": "^18.15.11",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.5",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "prettier": "^3.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.7",
    "typedoc-plugin-include-example": "^1.0.2",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.3.3",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EM-Enterprise/Woocommerce-Utils.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
